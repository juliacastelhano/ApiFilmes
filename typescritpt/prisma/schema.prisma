generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Filmes {
  id          Int        @id @default(autoincrement())
  titulo      String     @unique
  genero      String
  ano         Int
  estrelas    Int
  favoritos Favoritos[]
}

model Favoritos {
  id     Int      @id @default(autoincrement())
  titulo String   @unique
  Filmes   Filmes? @relation(fields: [filmesId], references: [id])
  filmesId Int?
}
